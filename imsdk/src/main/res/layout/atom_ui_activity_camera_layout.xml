<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              tools:context="com.qunar.im.ui.view.camera.CameraActivity">

    <com.qunar.im.ui.view.camera.view.QCameraView
        android:id="@+id/qcameraview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:duration_max="10000"
        app:iconMargin="20dp"
        app:iconLeft="@drawable/atom_ui_ic_back"
        app:iconSize="30dp"
        app:iconSrc="@drawable/atom_ui_ic_camera"
        />
</LinearLayout>
